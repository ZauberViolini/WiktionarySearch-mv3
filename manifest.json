{
  "name": "__MSG_name__",
  "version": "1.3.4",
  "manifest_version": 3,
  "description": "__MSG_desc__",
  "homepage_url": "https://github.com/ZauberViolini/WiktionarySearch-mv3",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "service_worker": "src/bg/background.js"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Wiktionary Search (mv3)",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "options_ui": {
    "page": "src/opt/options.html",
    "chrome_style": true
  },
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "host_permissions": [
    "*://*.wiktionary.org/*"
  ]
}
